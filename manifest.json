{
	"manifest_version": 2,
	"name": "NzFeral",
	"version": "2.0.0",
	"content_scripts": [
		{
			"matches": [
				"https://www.nzherald.co.nz/*", "http://www.nzherald.co.nz/"],
			"runat": "document_end",  
			"js": [
				"debugging.js",
				"app_storage.js",
				"unblocker.js",
				"unblocker_legacy.js",
				"app.js"
			]
		}
	],
	"browser_action": {
		"default_title": "NzFeral",
		"default_popup": "popup.html"
		},
	"permissions": [
		"tabs",
		"storage"
	]
}